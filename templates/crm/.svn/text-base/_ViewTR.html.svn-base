<table cellpadding="0" cellspacing="0" width="100%">
<tr valign="top">
    <td width="75px" style="padding: 0px 10px 0px 0px;">
        <div id="date">
        <span class="month">&nbsp;</span><br />
        <br />
        </div>
    </td>
    <td valign="top">
		<div id="ItemTitle">
			<h1>
            <a href="javascript: selectAjaxURL('{% url crm.views.view customer.entity_reference.key.id,customer.key.id %}');">
				{{customer.name|escape}}
			</a>
			</h1>
			<em>{{customer.created|date:"D"}}         {{customer.created|date:"d"}}         {{customer.created|date:"M"}}         {{customer.created|date:"Y"}} </em>
		</div>
    </td>
    <td width="150px">
        <a class="editbutton" href="javascript: selectAjaxURL('{% url crm.views.edit customer.entity_reference.key.id,customer.key.id  %}');">edit</a>
        <br />
        <br />
        <a class="deletebutton" href="{% url crm.views.delete customer.entity_reference.key.id,customer.key.id %}">delete</a>
    </td>
</tr>
</table>
<hr>
