    <!--  Dev -->
    <script type="text/javascript" src="/static/javascript/jquery/jquery-ui-1.7.2/js/jquery-1.3.2.min.js"></script>
    <script type="text/javascript" src="/static/javascript/jquery/jquery-ui-1.7.2/js/jquery-ui-1.7.2.custom.min.js"></script>

    <!-- JQuery CSS Theme -->
    <link rel="stylesheet" href="/static/javascript/jquery/jquery-ui-1.7.2/css/flick/jquery-ui-1.7.2.custom.css" type="text/css" />

    <!-- Core Soho Scheduler CSS -->
    <link rel="stylesheet" type="text/css" href="/static/stylesheets/sohocore.css" />


    <!-- JQuery Form Plugin to allow ajax form postbacks -->
    <script type="text/javascript" src="/static/javascript/jquery/jquery.form.js"></script>

    <!-- JQuery NyroModal Form -->
    <link rel="stylesheet" href="/static/javascript/jquery/nyroModal/styles/nyroModal.css" type="text/css" media="screen" />
    <script type="text/javascript" src="/static/javascript/jquery/nyroModal/js/jquery.nyroModal-1.5.0.pack.js"></script>

    <!-- FullCalendar -->
    <link rel='stylesheet' type='text/css' href='/static/javascript/jquery/fullcalendar/fullcalendar.css' />
    <script type='text/javascript' src='/static/javascript/jquery/fullcalendar/fullcalendar.min.js'></script>


    <!-- JMonth Corners -->
    <script type="text/javascript" src="/static/javascript/jquery/jquery.corner.js" ></script>

    <!-- JQuery List Nav Plugin Used in CRM Screen-->
    <script type="text/javascript" src="/static/javascript/jquery/jquery.listnav.min-2.0.js" ></script>

    <!-- Soho Scheduler Helper Code-->
    <script type="text/javascript" src="/static/javascript/soho_navigation.js"></script>

    <!-- Form Validator Engine -->
    <script type="text/javascript" src="/static/javascript/jquery/formValidator/js/jquery.validationEngine.js"></script>
    <link rel="stylesheet" href="/static/javascript/jquery/formValidator/css/validationEngine.jquery.css" type="text/css" media="screen" title="no title" charset="utf-8" />
    
    <!-- Json Library-->
    <script type="text/javascript" src="/static/javascript/jquery/jquery.json.js"></script>

    <!-- JBar -->
    <script type="text/javascript" src="/static/javascript/jquery/jquery.notifications/jquery.notifications-1.1.js"></script>
    <link rel="stylesheet" href="/static/javascript/jquery/jquery.notifications/jquery.notifications.css" type="text/css" media="screen" title="no title" charset="utf-8" />

    {% block headstyle %}{% endblock headstyle %}

    {% block headjs %}
    {% endblock headjs %}

    {% if redirect_to_url %}
        top.location.href = redirect_to_url;
    {% endif %}

    <script type="text/javascript" language="javascript">
        $(document).ready(function(){
            $('a.disabled').fadeTo("slow", 0.5);

            //Make Menu Bar touchy feely
            var navDuration = 150; //time in miliseconds
            var navJumpHeight = "0.15em";
            $('#mainHorizTabs li').not("li.disabled").hover(function() {
                $(this).animate({ top : "-="+navJumpHeight }, navDuration);
            }, function() {
              $(this).animate({ top : "0px" }, navDuration);
            });
			
			{%if entity_id%}
            $('#header_bar_logo').click(function(){top.location.href='{%url dashboard.views.index entity_id%}';})
			{%endif%}

             var message_text = '{{request.session.temp_message_body}}';
             var message_type = '{{request.session.temp_message_type}}';
            ShowNotification(message_text,message_type);
    });
    </script>
</head>
<body>
<div id="debug_info" >
</div>
