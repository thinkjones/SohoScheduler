    GSFN.feedback('http://getsatisfaction.com/sohoscheduler/feedback/topics/new?display=overlay&style=idea', tab_options);
    //GSFN.feedback('http://getsatisfaction.com/sohoscheduler/feedback/topics/new?display=overlay&style=idea', {container:'divFeedbackPlaceholder'});
    </script>

