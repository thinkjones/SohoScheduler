<div id="divAllContent">
	<div style="display:none;">
	  {% ifnotequal entity_id "0" %}
		  <h1>{{entity.desc }}</h1>
		  <h1>{{entity.tags}}</h1>
		{% endifnotequal %}
	</div>

    <!-- Header -->
	<div id="header_bar">
            <div id="header_bar_logo">
            </div>
		{%include "_right_hand_links.html" %}
		<!--<h1 class="{% if is_design_mode %}designmode{% else %}entitymode{% endif %}">{{ header_text }}</h1>-->
	</div>

    {% if asdasdas %}
        Tab: {{ tabSelected }} App Version:  {{ application_version }} Entity ID: {{entity_id}} is_admin_screen: {{is_admin_screen}}  header_text:{{ header_text }} EntityCount:{{ EntityCount }}
    {% endif %}
    
    <div id="navigation">
            {% if is_admin_screen %}
                {%include "_menu_admin.html" %}
            {% else %}
                {%include "_menu_public.html" %}
            {% endif %}
    </div>

    {%block submenu %}
    {%endblock submenu %}

    {% ifnotequal entity_id "0" %}
        {% include "message/maincontentmessages.html" %}
    {% endifnotequal %}
    <div id="divMainContent">
        {%block maincontent %}
        {%endblock maincontent %}
    </div>
    
    <hr class="clearboth"/>

    {%include "about/_footer.html" %}

    <div id="divFeedbackPlaceholder">
    </div>
</div>


{% if asdasddas %}
    <!-- Get Satisfaction Additions -->
    <style type='text/css'>@import url('http://s3.amazonaws.com/getsatisfaction.com/feedback/feedback.css');</style>
    <script src='http://s3.amazonaws.com/getsatisfaction.com/feedback/feedback.js' type='text/javascript'></script>

    <script type="text/javascript" charset="utf-8">
    var tab_options = {}
    tab_options.placement = "bottom";  // left, right, bottom, hidden
    tab_options.color = "#222"; // hex (#FF0000) or color (red)
